    "@types/is": "^0.0.21",
    "@types/lodash": "^4.14.132",
    "@types/node": "^12.0.2",
    "@types/prettier": "^1.16.4",
    "@types/react": "^16.8.18",
    "@types/react-dom": "^16.8.4",
    "@types/react-transition-group": "^2.9.1",
    "antd": "^3.18.2",
    "cache-loader": "^3.0.1",
    "css-loader": "^2.1.1",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "file-loader": "^3.0.1",
    "fork-ts-checker-webpack-plugin": "^1.3.4",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-tags-plugin": "^2.0.12",
    "http-server": "^0.11.0",
    "immer": "3.1.2",
    "prettier": "^1.17.1",
    "react": "*",
    "react-dom": "*",
    "style-loader": "^0.23.1",
    "thread-loader": "^2.1.2",
    "ts-loader": "^6.0.1",
